{
	"name": "Tab Creator",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Opens a new specified tab",

	"browser_action": {
		"default_title": "Tab Creator",
		"default_popup": "box.html"
	},

    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+1",
                "mac": "Command+Shift+1",
                "chromeos": "Ctrl+Shift+1",
                "linux": "Ctrl+Shift+1"
        
            }
        }
    },
	
	"permissions": [
		"tabs",
		"windows"
	]
}
